<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uds.pautando">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".features.forgot_password.presentation.pages.ForgotPasswordPage"></activity>
        <activity android:name=".features.sign_up.presentation.pages.SignUpPage" />
        <activity android:name=".features.home.presentation.pages.HomeActivity" />
        <activity android:name=".features.sign_in.presentation.pages.LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>